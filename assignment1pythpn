   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# --- 1. Read the CSV file into a NumPy array ---\n",
    "# Assume the CSV has columns like: Gender, Height, Weight\n",
    "data = np.genfromtxt('weight-height.csv', delimiter=',', skip_header=1)\n",
    "\n",
    "# --- 2. Cut the table into lengths and weights ---\n",
    "length = data[:, 1]   # Height in inches\n",
    "weight = data[:, 2]   # Weight in pounds\n",
    "\n",
    "# --- 3. Convert units ---\n",
    "length_cm = length * 2.54         # inches  centimeters\n",
    "weight_kg = weight * 0.45359237   # pounds  kilograms\n",
    "\n",
    "# --- 4. Calculate means ---\n",
    "mean_length = np.mean(length_cm)\n",
    "mean_weight = np.mean(weight_kg)\n",
    "\n",
    "print(f\"Average height: {mean_length:.2f} cm\")\n",
    "print(f\"Average weight: {mean_weight:.2f} kg\")\n",
    "\n",
    "# --- 5. Draw a histogram of the lengths ---\n",
    "plt.figure(figsize=(8, 6))\n",
    "plt.hist(length_cm, bins=20, color='skyblue', edgecolor='black')\n",
    "plt.title('Histogram of Student Lengths')\n",
    "plt.xlabel('Length (cm)')\n",
    "plt.ylabel('Frequency')\n",
    "plt.grid(True, linestyle='--', alpha=0.6)\n",
    "plt.show()"
   ]
  },
